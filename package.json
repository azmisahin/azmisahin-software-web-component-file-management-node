{
  "name": "file-management-simple",
  "version": "0.0.6",
  "description": "Simple File Management",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint src --fix && eslint tests --fix",
    "test": "node tests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
